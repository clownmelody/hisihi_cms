@-webkit-keyframes tipmove {
  0% {
    bottom: 10px;
    opacity: 0;
  }
  50% {
    bottom: 15px;
    opacity: 1;
  }
  100% {
    bottom: 20px;
    opacity: 0;
  }
}
@-webkit-keyframes tipmove-left {
  0% {
    -webkit-transform: translateX(10px);
    opacity: 0;
  }
  50% {
    -webkit-transform: translateX(5px);
    opacity: 1;
  }
  100% {
    -webkit-transform: translateX(0);
    opacity: 0;
  }
}
@-webkit-keyframes tipmove-right {
  0% {
    -webkit-transform: translateX(-10px);
    opacity: 0;
  }
  50% {
    -webkit-transform: translateX(-5px);
    opacity: 1;
  }
  100% {
    -webkit-transform: translateX(0);
    opacity: 0;
  }
}
