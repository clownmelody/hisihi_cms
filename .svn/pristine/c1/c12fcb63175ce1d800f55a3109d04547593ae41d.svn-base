#balloon {
  width: 64px;
  height: 86px;
  background: url("../../img/lightgames/dots_64x86.png") no-repeat 0 0;
  -webkit-transition: top 1s ease-out, left 1s ease-out;
  transition: top 1s ease-out, left 1s ease-out;
  -webkit-animation: red-balloon 1.5s step-end 0s infinite;
  animation: red-balloon 1.5s step-end 0s infinite;
}
#balloon.moving {
  background-position: -896px -602px;
  -webkit-transition: all 0s;
  transition: all 0s;
  -webkit-animation: red-balloon-moving 0.2s step-end 0s infinite;
  animation: red-balloon-moving 0.2s step-end 0s infinite;
}
