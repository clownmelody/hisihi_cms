  background-color: rgba(0, 0, 0, 0.9);
  z-index: -1;
  opacity: 0;
}
.active {
  opacity: 1;
  z-index: 110;
}
@keyframes loadAni {
  from {
    opacity: 1;
    display: block;
  }
  to {
    opacity: 0;
    display: none;
  }
}
@-webkit-keyframes loadAni {
  from {
    opacity: 1;
    display: block;
  }
  to {
    opacity: 0;
    display: none;
  }
}
div.loadingVip {
  z-index: 1000001;
  position: absolute;
  top: 0;
  left: 0;
  right: 0;
  bottom: 0;
  background: #fff;
  animation: loadAni 3s ease-in forwards;
  -webkit-animation: loadAni 3s ease-in forwards;
}
div.loadingVip div.loadingImg {
  background-size: 100px 100px;
  position: absolute;
  width: 100px;
  height: 100px;
  top: 35%;
  left: 50%;
  margin-top: -50px;
  margin-left: -50px;
}
div.loadingVip div.loadingImg div.loadingText {
  margin-top: 100px;
  text-align: center;
}
