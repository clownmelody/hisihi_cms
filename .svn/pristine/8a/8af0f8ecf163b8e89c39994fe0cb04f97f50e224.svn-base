        function showQrCode(){
            var qr = $(qrcodeSelector);
            if (!qr.get(0)) {
                initQrCode();
            } else {
                qr.show();
            }
        }
        function initQrCode(){
            if (!$.fn.appendTo) {
                $.fn.appendTo = function(){
                    var p = arguments[0] || document.body;
                    $(p).append(this);
                    return $(p).children().last();
                };
            }
            if (!$.fn.qrcode) {
                loadScripts([
                    //"../lib/jquery.min.js",
                    "../lib/vue.min.js",
                    "../lib/quo.qrcode.js",
                    "../lib/qrcode.js"
                ], createQrCode);
            } else {
                createQrCode();
            }
        }
        function createQrCode(){
            var qr = $(document.body).append('<div id="'+qrcodeId+'" style="padding-top: '+(document.documentElement.clientHeight-192)/2+'px;"></div>').find(qrcodeSelector);
